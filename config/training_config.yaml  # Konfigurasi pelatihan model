  provider_priority:
    chain:
      - name: "cypher"
        endpoint: "https://api.cypher.ai/v1/completions"
        model: "cypher-alpha"
        credentials:
          key: "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"
