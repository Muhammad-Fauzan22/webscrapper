    time_slice:
      count: 24
      distribution: "sin(i / 24 * Ï€)"
      provider_mapping:
        - time: "00:00-06:00"
          provider: "cypher"
        - time: "06:00-12:00"
          provider: "deepseek"
        - time: "12:00-18:00"
          provider: "claude"
        - time: "18:00-24:00"
