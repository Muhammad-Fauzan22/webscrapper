      cypher: 1
      deepseek: 2
      claude: 3
      fallback: 4
    weight_adjustment:
      daily: "auto"
      strategy: "sinusoidal"
      amplitude: 0.5
      frequency: 1.0
    time_slice:
      count: 24
      distribution: "sin(i / 24 * π)"
      provider_mapping:
